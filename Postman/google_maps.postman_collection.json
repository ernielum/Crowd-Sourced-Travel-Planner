{
    "info": {
      "_postman_id": "unique_identifier_for_google_maps_collection",
      "name": "Google Maps API Integration",
      "description": "A collection to interact with Google Maps API for searching and retrieving place details.",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Search Maps",
        "request": {
          "method": "GET",
          "url": "{{url}}/map/search?query={{query}}&location={{location}}",
          "header": [],
          "description": "Search for businesses via Google Maps API based on user query or map location."
        },
        "response": []
      },
      {
        "name": "Get Place Details",
        "request": {
          "method": "GET",
          "url": "{{url}}/map/details/{{placeId}}",
          "header": [],
          "description": "Get detailed information about a place from Google Maps."
        },
        "response": []
      }
    ],
    "variable": [
      {
        "key": "url",
        "value": "http://your_api_domain_here"
      },
      {
        "key": "query",
        "value": "search_query_here"
      },
      {
        "key": "location",
        "value": "latitude,longitude"
      },
      {
        "key": "placeId",
        "value": "google_place_id_here"
      }
    ]
  }